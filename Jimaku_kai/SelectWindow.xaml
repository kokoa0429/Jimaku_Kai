<Window x:Class="Jimaku_kai.SelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Jimaku_kai"
        mc:Ignorable="d"
        Title="SelectWindow" Height="100" Width="270" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="Combo" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="133" Height="49" FontSize="18"  ItemsSource="{Binding Layout.Value}" />
        <Button Content="実行" HorizontalAlignment="Left" Margin="163,18,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
    </Grid>
</Window>
